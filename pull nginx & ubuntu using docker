pull nginx image using cluster

create instance allow http-80, custom tcp 8080

yum install docker
systemctl start docker
systemctl enable docker
docker pull nginx
docker run -it --name nginx-app -p 8080:80 nginx:latest /bin/bash
ls
cd usr/share/nginx
cd html
service nginx start

paste the ip:8080 on new tab



docker pull ubuntu
docker run -it --name ubuntu-app -p 8080:80 ubuntu:latest /bin/bash
apt update -y
apt install apache2
cd /var/www/html/
 echo "">index.html
service apache2 start 
paste the ip:8080 on new tab
